{
  "name": "house-of-isometry",
  "version": "1.0.0",
  "description": "Isometric projection in the DOM",
  "main": "dist/house-of-isometry.js",
  "engines": {
    "node": "7.2.0",
    "npm": "3.10.3",
    "yarn": "0.17.4"
  },
  "scripts": {
    "build": "npm run build-js & npm run build-css",
    "watch": "npm run watch-js & npm run watch-css",
    "build-js": "babel src/house-of-isometry.js --out-file dist/house-of-isometry.js",
    "watch-js": "babel --watch src/house-of-isometry.js --out-file dist/house-of-isometry.js",
    "build-css": "node-sass --include-path scss src/house-of-isometry.scss dist/house-of-isometry.css",
    "watch-css": "node-sass -w --include-path scss src/house-of-isometry.scss dist/house-of-isometry.css"
  },
  "repository": "https://github.com/antistatique/house-of-isometry",
  "author": "Julien Minguely <jminguely@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-watch": "^2.0.6",
    "babelify": "^7.3.0",
    "browserify": "^14.3.0",
    "node-sass": "^4.5.2"
  },
  "devDependencies": {}
}
